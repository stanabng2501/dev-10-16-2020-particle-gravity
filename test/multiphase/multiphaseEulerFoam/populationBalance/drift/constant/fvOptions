FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "constant";
    object      fvOptions;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

options
{
    massSource
    {
        type            semiImplicitSource;

        selectionMode   all;
        volumeMode      specific;

        sources
        {
            thermo:rho.air
            {
                explicit    0.024535; // kg/s/m^3
                implicit    0;
            }
            thermo:rho.water
            {
                explicit    -24.535; // kg/s/m^3
                implicit    0;
            }
        }
    }
}


