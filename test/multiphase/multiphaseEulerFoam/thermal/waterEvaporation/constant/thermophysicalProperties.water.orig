FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "constant";
    object      thermophysicalProperties.water;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

thermoType
{
    type            heRhoThermo;
    mixture         pureMixture;
    transport       const;
    thermo          eConst;
    equationOfState rhoConst;
    specie          specie;
    energy          sensibleInternalEnergy;
}

mixture
{
    specie
    {
        molWeight   18.0153;
    }
    equationOfState
    {
        rho         1027;
    }
    thermodynamics
    {
        Hf          -1.58643e+07;
        Cv          4195;
        Cp          4195;
        Tref        372.76;
        Hsref       312508;
        Esref       312410;
    }
    transport
    {
        mu          2.8291e-4;
        Pr          2.289;
    }
}

